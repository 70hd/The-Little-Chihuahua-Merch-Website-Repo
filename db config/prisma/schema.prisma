generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Size {
  id        Int          @id @default(autoincrement())
  size      String
  productId Int
  inventory Int
  status    Size_status?
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Image {
  id        Int    @id @default(autoincrement())
  image     String
  productId Int
  alt       String
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Prices {
  id        Int @id @default(autoincrement())
  price     Int
  productId Int
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Product {
  id          Int             @id @default(autoincrement())
  title       String          @unique
  description String
  colorHex    String
  colorName   String
  status      Product_status?
  inventory   Int
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model RestockNotification {
  id        Int    @id @default(autoincrement())
  email     String
  productId Int
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Contact {
  id             Int     @id @default(autoincrement())
  name           String
  email          String
  phoneNumber    String?
  selectLocation String?
  comments       String
}

enum Size_status {
  AVAILABLE
  LOW_STOCK
  OUT_OF_STOCK
}

enum Product_status {
  AVAILABLE
  LOW_STOCK
  OUT_OF_STOCK
}
